@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<#Commodity> rml:logicalSource [ rml:iterator "$.[*].fields" ;
            rml:referenceFormulation ql:JSONPath ;
            rml:source "commodity-prices@public" ] ;
    rr:predicateObjectMap [ rr:objectMap [ rml:reference "$.date" ;
                    rr:datatype xsd:date ] ;
            rr:predicate <http://purl.org/dc/terms/modified> ],
        [ rr:objectMap [ rml:reference "$.commodity" ] ;
            rr:predicate rdfs:label ],
        [ rr:objectMap [ rml:reference "$.price_index" ;
                    rr:datatype xsd:double ] ;
            rr:predicate <http://dbpedia.org/ontology/price> ];
    rr:subjectMap [ rr:class <https://w3id.org/seas/Commodity> ;
            rr:template "https://data.opendatasoft.com/ld/resources/commodity-prices@public/Commodity/{commodity}" ] .
