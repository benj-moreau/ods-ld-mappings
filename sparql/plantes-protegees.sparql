PREFIX schema: <http://schema.org/>
PREFIX dbo: <http://dbpedia.org/ontology/>
PREFIX dbp: <http://dbpedia.org/property/>
PREFIX dbr: <http://dbpedia.org/resource/>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>
PREFIX wlo: <http://purl.org/ontology/wo/>
PREFIX dcmi: <http://purl.org/dc/terms/>
PREFIX eli: <http://data.europa.eu/eli/ontology#>

SELECT ?nom_plante ?description ?image ?espece ?article_protection
WHERE {
  ?s foaf:name ?nom_plante .
  ?s wlo:conservationStatus ?cs .
  ?cs schema:identifier ?article_protection .
  ?s dbo:kingdom dbr:Plant .
  ?s foaf:depiction ?image .
  ?s rdfs:comment ?description .
  ?s dbp:species ?espece
}
